<?xml version="1.0" encoding="UTF-8"?>
<protocols-config distPath="src/main/java"
	packageName="com.psp.provider.controller" resPackageName="res/bean"
	reqPackageName="req" xmlPath="src/main/java/com/psp/provider/protocols">

	<protocolFiles>
		<protocols-file fileName="account-protocol.xml" toCreate="true"
			className="AccountController" requestMapping="/account" version="1.0"
			notes="账户相关接口" />
		<protocols-file fileName="order-protocol.xml" toCreate="true"
			className="OrderController" requestMapping="/order" version="1.0"
			notes="工单相关接口" />
	</protocolFiles>

	<beanFiles>
		<bean-file fileName="beans.xml" />
	</beanFiles>

	<resBean resCode="rescode" resMsg="msg" resData="data"
		toCreate="false" />

</protocols-config>