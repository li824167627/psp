<?xml version="1.0" encoding="UTF-8"?>
<protocols-config distPath="src/main/java"
	packageName="com.psp.admin.controller" resPackageName="res/bean"
	reqPackageName="req" xmlPath="src/main/java/com/psp/admin/protocols">

	<protocolFiles>
		<protocols-file fileName="admin-protocol.xml" toCreate="true"
			className="AdminController" requestMapping="/admin" version="1.0"
			notes="管理员相关接口" />
		<protocols-file fileName="user-protocol.xml" toCreate="true"
			className="UserController" requestMapping="/user" version="1.0"
			notes="用户相关接口" />
		<protocols-file fileName="provider-protocol.xml" toCreate="true"
			className="ProviderController" requestMapping="/provider" version="1.0"
			notes="服务商相关接口" />
	</protocolFiles>

	<beanFiles>
		<bean-file fileName="beans.xml" />
	</beanFiles>

	<resBean resCode="rescode" resMsg="msg" resData="data"
		toCreate="false" />

</protocols-config>