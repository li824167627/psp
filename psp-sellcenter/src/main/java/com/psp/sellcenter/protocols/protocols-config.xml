<?xml version="1.0" encoding="UTF-8"?>
<protocols-config distPath="src/main/java"
	packageName="com.psp.sellcenter.controller" resPackageName="res/bean"
	reqPackageName="req" xmlPath="src/main/java/com/psp/sellcenter/protocols">

	<protocolFiles>
		<protocols-file fileName="seller-protocol.xml" toCreate="true"
			className="SellController" requestMapping="/seller" version="1.0"
			notes="销售相关接口" />
			
		<protocols-file fileName="user-protocol.xml" toCreate="true"
			className="UserController" requestMapping="/user" version="1.0"
			notes="客户相关接口" />
			
		<protocols-file fileName="usernews-protocol.xml" toCreate="true"
			className="UserNewsController" requestMapping="/usernews" version="1.0"
			notes="客户消息流相关接口" />
			
		<protocols-file fileName="order-protocol.xml" toCreate="true"
			className="OrderController" requestMapping="/order" version="1.0"
			notes="工单相关接口" />
			
	</protocolFiles>
	
	<beanFiles>
		<bean-file fileName="beans.xml" />
	</beanFiles>

	<resBean resCode="rescode" resMsg="msg" resData="data"
		toCreate="false" />

</protocols-config>